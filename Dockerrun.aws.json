{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "vsprakash2003/docker-multi-container-client-test:latest",
            "hostname": "client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "vsprakash2003/docker-multi-container-server-test:latest",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "image": "vsprakash2003/docker-multi-container-worker-test:latest",
            "hostname": "worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "vsprakash2003/docker-multi-container-nginx-test:latest",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["server", "client"],
            "memory": 128
        }
    ]
}